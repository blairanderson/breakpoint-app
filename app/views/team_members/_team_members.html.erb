<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone Number</th>
    </tr>
  </thead>
  <tbody>
    <% team_members.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= mail_to user.email %></td>
      <td><%= user.phone_number %></td>
    </tr>
    <% end %>
  </tbody>
</table>

